{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": [
    "**/*.svg",
    "**/*.png",
    "**/*.jpg",
    "**/*.pdf",
    "**/node_modules/**",
    "**/.git/**"
  ],
  "words": [
    "OAuth",
    "OIDC",
    "AuthZ",
    "AuthN",
    "workflow",
    "workflows",
    "access token",
    "access"
  ],
  "enableFiletypes": ["md", "txt"]
}
